{
  "name": "colyseus.js",
  "version": "0.14.1",
  "description": "Colyseus Multiplayer Client for JavaScript/TypeScript",
  "author": "Endel Dreyer",
  "license": "MIT",
  "scripts": {
    "test": "mocha test/*.ts --require ts-node/register",
    "build": "rollup -c rollup.config.js",
    "build-dev": "webpack",
    "build-dist-dts": "dts-bundle-generator --config dts-generator.json",
    "build-all": "npm run build && npm run build-dev",
    "watch": "tsc -w",
    "tslint": "tslint --project .",
    "prepublishOnly": "tsc && npm run build"
  },
  "files": [
    "build",
    "lib",
    "LICENSE",
    "README.md"
  ],
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "browser": "build/colyseus.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "browser": "./build/colyseus.js",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.js"
    }
  },
  "keywords": [
    "colyseus",
    "multiplayer",
    "netcode",
    "realtime",
    "networking",
    "websockets"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.js.git"
  },
  "engines": {
    "node": ">= 12.x"
  },
  "dependencies": {
    "@colyseus/schema": "^1.0.0",
    "httpie": "^2.0.0-next.13",
    "nanoevents": "^5.1.5",
    "notepack.io": "^2.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.44",
    "benchmark": "^2.1.4",
    "chai": "^3.5.0",
    "install": "^0.12.2",
    "mocha": "^5.1.1",
    "nanoid": "^2.0.1",
    "node-localstorage": "^1.3.1",
    "npm": "^6.9.0",
    "rollup": "^2.40.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^6.0.3",
    "tslint": "^5.9.1",
    "typescript": "^4.2.2",
    "ws": "^6.1.2"
  }
}
