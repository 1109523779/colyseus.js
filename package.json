{
  "name": "colyseus.js",
  "version": "0.3.0",
  "description": "JavaScript client for Colyseus Game Server",
  "keywords": [
    "multiplayer",
    "mmo",
    "networking",
    "websockets"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/gamestdio/colyseus.js.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run dist-release && nodemon --harmony ./examples/server.js",
    "watch-release": "watchify ./src/index.js -t babelify -s Colyseus -o dist/colyseus.js",
    "dist-release": "browserify ./src/index.js -t babelify -s Colyseus -o dist/colyseus.js",
    "dist-uglify": "uglifyjs dist/colyseus.js --compress --mangle > dist/colyseus.min.js",
    "dist": "npm run dist-release && npm run dist-uglify",
    "watch": "babel src -d lib -w",
    "prepublish": "babel src -d lib"
  },
  "engines": {
    "node": ">= 5.x"
  },
  "dependencies": {
    "fast-json-patch": "^0.5.4",
    "msgpack-lite": "^0.1.20",
    "websocket.js": "^0.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "browserify": "^12.0.1",
    "babelify": "^7.2.0",
    "uglify-js": "^2.6.1",
    "colyseus": "^0.4.0",
    "express": "^4.13.3",
    "nodemon": "^1.7.1",
    "watchify": "^3.6.1"
  }
}
